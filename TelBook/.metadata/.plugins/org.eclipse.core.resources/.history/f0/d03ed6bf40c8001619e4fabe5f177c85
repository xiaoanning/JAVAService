package util;

import java.io.InputStream;
import java.util.Properties;

public class JDBCUtil 
{
	private static Properties properties = null ;
	static
	{
		InputStream is = null ;
		
		try
		{
			is = JDBCUtil.class.getResourceAsStream("/Person.properties");
			properties.load(is);
			
		}catch(Exception e)
		{
			e.printStackTrace();
		}finally
		{
			
		}
	}
}
